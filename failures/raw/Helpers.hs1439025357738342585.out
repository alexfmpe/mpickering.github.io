  (Ann (DP (0,0)) (ColDelta 1) DP (0,0) [] [((AnnComment DComment (DP (0,0),DP (0,36)) "{-# LANGUAGE ScopedTypeVariables #-}" Nothing),DP (0,0)),((G AnnModule),DP (1,1)),((G AnnVal),DP (0,1)),((AnnComment DComment (DP (1,5),DP (1,123)) "-- ( smartserver, exactdir, smartserver',getData',vhosts,vhost, cleanUpLockFile, scrapeUrl, websiteUp, stateShutDown )" Nothing),DP (1,5)),((G AnnWhere),DP (2,3)),((AnnComment DComment (DP (3,1),DP (3,136)) "-- REMOVED. The better way to solve this problem is to use simpleHTTPWithWebSocket, which doesn't prevent startup on kill-9 that leaves" Nothing),DP (3,1)),((AnnComment DComment (DP (1,1),DP (1,25)) "--  ghost socket around." Nothing),DP (1,1)),((AnnComment DComment (DP (1,1),DP (4,3)) "{-| If a forked IO action throws an exception within a certain amount of time, return the exception along with the threadId.\n    wait time needs to be long enough for the exception to arise, so when in doubt, err on the side of longer wait time\n\n-}" Nothing),DP (1,1)),((AnnComment DComment (DP (1,1),DP (9,3)) "{-\ncatchStartupErrorInForkIO :: IO () -> Int -> IO (ThreadId, Either E.SomeException ())\ncatchStartupErrorInForkIO ioAction waitSeconds = do\n  eIOref <- newIORef $ Right ()\n  tid <- forkIO $ ioAction `E.catch` (\\e -> writeIORef eIOref (Left e))\n  threadDelay $ waitSeconds * (10^6)\n  new <- readIORef eIOref\n  return (tid,new)\n-}" Nothing),DP (1,1)),((AnnComment DComment (DP (2,1),DP (2,136)) "-- REMOVED. The better way to solve this problem is to use simpleHTTPWithWebSocket, which doesn't prevent startup on kill-9 that leaves" Nothing),DP (2,1)),((AnnComment DComment (DP (1,1),DP (1,25)) "--  ghost socket around." Nothing),DP (1,1)),((AnnComment DComment (DP (1,1),DP (1,8)) "-- WAS:" Nothing),DP (1,1)),((AnnComment DComment (DP (1,1),DP (1,108)) "-- This is useful if you have a web server that crashes, but sometimes leaves the lock file hanging around." Nothing),DP (1,1)),((AnnComment DComment (DP (1,1),DP (1,147)) "-- But sometimes, due to unknown reasons, has a false restart that should quickly exit because the lock file exists and the app is really running." Nothing),DP (1,1)),((AnnComment DComment (DP (1,1),DP (1,112)) "-- What this function does is clean up the lock file, but only if a curl request for the actual web site fails." Nothing),DP (1,1)),((AnnComment DComment (DP (1,1),DP (1,131)) "-- This feels like risky business, try to find a better way (unclear on what the race condition, if any, is exactly but uneasy...)" Nothing),DP (1,1)),((AnnComment DComment (DP (1,1),DP (1,137)) "-- Another danger is, what if the generated lock file doesn't match the actual lock file? (like if the underlying library call changes?)" Nothing),DP (1,1)),((AnnComment DComment (DP (1,1),DP (24,3)) "{-\ncleanUpLockFile thisprog checkurl teststring waitSecs = do\n  progName <- getProgName\n  -- prevents inadvertent running from inside ghci.\n  unless (thisprog == progName) $ error $ \"prog name isn't \" ++ progName ++  \" exiting: \" ++ progName\n\n  lockFile <- liftM (</> \"_local\" </> progName ++ \"_state.lock\") getCurrentDirectory\n  putStrLn $ \"cleanUpLockFile, lock file: \" ++ lockFile\n  lock_fileExists <- doesFileExist lockFile\n  when lock_fileExists $ do\n      -- Q: What if website really is up, but slower to respond than waitSecs.\n      -- A: The lock would be deleted and the app would attempt to start\n      --    It would probably fail because port is blocked.\n      --    It would keep trying to start because that's what supervisord does.\n      -- Q. Can I conceive of scenario where website is up and a second server process is attempted to be started,\n      --      when daemonized under supervisord?\n      -- A. Not at the moment.\n      up <- websiteUp waitSecs checkurl teststring\n      if up\n        then do\n          threadDelay $ 2 * 10^6\n          error $ \" cleanUpLockFile: \" ++ progName ++ \" is already running. That's odd. Exiting now.\"\n        else removeFile lockFile\n-}" Nothing),DP (1,1)),((AnnComment DComment (DP (2,1),DP (2,136)) "-- REMOVED. The better way to solve this problem is to use simpleHTTPWithWebSocket, which doesn't prevent startup on kill-9 that leaves" Nothing),DP (2,1)),((AnnComment DComment (DP (1,1),DP (1,25)) "--  ghost socket around." Nothing),DP (1,1)),((AnnComment DComment (DP (1,1),DP (1,8)) "-- WAS:" Nothing),DP (1,1)),((AnnComment DComment (DP (1,1),DP (23,3)) "{-\nwebsiteUp secsWaitRequest url teststring = do\n  let timeoutSecs n = timeout $ (10^6) * n\n  mbEtR <- timeoutSecs secsWaitRequest $ runErrorT $ scrapeUrl url\n  case mbEtR of\n    Nothing -> do putStrLn $ \"scrapeUrl returned nothing after \" ++ (show secsWaitRequest) ++ \"seconds, so we conclude it's donwn\"\n                  return False\n    Just etR -> case etR of\n      Left e -> return False\n      Right s -> return $ isInfixOf (lc teststring) (lc s)\n  where lc = map toLower\nscrapeUrl :: String -> ErrorT String IO String\nscrapeUrl url = do\n     rq <- ErrorT $ case Network.URI.parseURI url of\n                Nothing -> return . Left $ \"bad url: \" ++ url\n                Just uri -> return . Right $ HTTP.Request uri HTTP.GET [] \"\"\n\n     ErrorT $ do r <- catch ( HTTP.simpleHTTP rq )\n                            ( \\e -> return . Left . Network.Stream.ErrorMisc . show $ e)\n                 case r of\n                   Right x -> return . Right . HTTP.rspBody $ x\n                   Left y -> return . Left $ \"web fetch failed: \" ++ (show rq)\n-}" Nothing),DP (1,1)),((G AnnEofPos),DP (80,0))])),
